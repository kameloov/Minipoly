<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="com.minipoly.android.popup.PopupNew.NewType" />

        <variable
            name="pop"
            type="com.minipoly.android.popup.PopupNew" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout25"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/pop_bg"
        android:padding="8dp">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline79"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/pop_bg"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <ImageView
            android:id="@+id/imageView88"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="4dp"
            android:adjustViewBounds="true"
            android:background="@drawable/add_advrt_bg"
            android:clickable="true"
            android:onClick="@{()->pop.select(NewType.NT_DEAL)}"
            android:padding="4dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline79"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/overlay_red" />

        <ImageView
            android:id="@+id/imageView89"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:adjustViewBounds="true"
            android:background="@drawable/add_advrt_bg"
            android:clickable="true"
            android:onClick="@{()->pop.select(NewType.NT_AUCTION)}"
            android:padding="4dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/guideline79"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/overlay_blue" />

        <TextView
            android:id="@+id/textView175"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="15 M"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView92"
            app:layout_constraintEnd_toEndOf="@+id/imageView89"
            app:layout_constraintStart_toStartOf="@+id/guideline81"
            app:layout_constraintTop_toTopOf="@+id/imageView92" />

        <ImageView
            android:id="@+id/imageView90"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:adjustViewBounds="true"
            android:clickable="true"
            android:onClick="@{()->pop.select(NewType.NT_AD)}"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/imageView89"
            app:layout_constraintStart_toStartOf="@+id/imageView88"
            app:layout_constraintTop_toBottomOf="@+id/imageView89"
            app:srcCompat="@drawable/add_promoed_ad_bg" />

        <TextView
            android:id="@+id/textView171"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/new_deal"
            android:textColor="@color/gradiant_end"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView88"
            app:layout_constraintEnd_toEndOf="@+id/imageView88"
            app:layout_constraintHorizontal_bias="0.2"
            app:layout_constraintStart_toStartOf="@+id/imageView88"
            app:layout_constraintTop_toTopOf="@+id/imageView88"
            app:layout_constraintVertical_bias="0.2" />

        <TextView
            android:id="@+id/textView172"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="12dp"
            android:text="@string/new_ad"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/imageView90"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toStartOf="@+id/imageView90"
            app:layout_constraintTop_toTopOf="@+id/imageView90"
            app:layout_constraintVertical_bias="0.1" />

        <TextView
            android:id="@+id/textView173"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/new_auction"
            android:textColor="@color/blue_gradiant_end"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView89"
            app:layout_constraintEnd_toEndOf="@+id/imageView89"
            app:layout_constraintHorizontal_bias="0.2"
            app:layout_constraintStart_toStartOf="@+id/imageView89"
            app:layout_constraintTop_toTopOf="@+id/imageView89"
            app:layout_constraintVertical_bias="0.2" />

        <ImageView
            android:id="@+id/imageView91"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:adjustViewBounds="true"
            app:layout_constraintBottom_toBottomOf="@+id/imageView88"
            app:layout_constraintEnd_toStartOf="@+id/guideline80"
            app:layout_constraintStart_toStartOf="@+id/imageView88"
            app:srcCompat="@drawable/go" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline80"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.25" />

        <TextView
            android:id="@+id/textView174"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="15 M"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView91"
            app:layout_constraintEnd_toEndOf="@+id/imageView88"
            app:layout_constraintStart_toStartOf="@+id/guideline80"
            app:layout_constraintTop_toTopOf="@+id/imageView91" />

        <TextView
            android:id="@+id/textView177"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:text="12 K"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView90"
            app:layout_constraintStart_toStartOf="@+id/textView172" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline81"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.75" />

        <ImageView
            android:id="@+id/imageView92"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:adjustViewBounds="true"
            app:layout_constraintBottom_toBottomOf="@+id/imageView89"
            app:layout_constraintEnd_toStartOf="@+id/guideline81"
            app:layout_constraintStart_toStartOf="@+id/imageView89"
            app:srcCompat="@drawable/auction_btn" />

        <ImageView
            android:id="@+id/imageView93"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginEnd="8dp"
            android:adjustViewBounds="true"
            app:layout_constraintBottom_toBottomOf="@+id/textView172"
            app:layout_constraintEnd_toEndOf="@+id/imageView90"
            app:layout_constraintTop_toTopOf="@+id/textView172"
            app:srcCompat="@drawable/speaker" />

        <TextView
            android:id="@+id/textView176"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:text="@string/ads"
            android:textColor="@color/quantum_googredA700"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/imageView93"
            app:layout_constraintEnd_toStartOf="@+id/imageView93"
            app:layout_constraintTop_toTopOf="@+id/imageView93" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>